{
    "$schema": "http://json.schemastore.org/swcrc",
    "minify": false,
    "jsc": {
        "minify": {
            "compress": {
                "unused": true
            },
            "mangle": true
        },
        "parser": {
            "syntax": "ecmascript",
            "dynamicImport": true,
            "decorators": true
        },
        "target": "es2020",
        "baseUrl": "."
    },
    "module": {
        "type": "commonjs"
    }
}
